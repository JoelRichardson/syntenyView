#!/bin/bash

source config.sh

declare -a arr=(`ls ${DIR}/data/*.gff3`)

lim=$((${#arr[@]} - 1))

#!/bin/bash
for i in `seq 0 ${lim}`; do
    jstart=$(($i+1))
    if [ ${jstart} -le ${lim} ]; then
        for j in `seq $(($i+1)) ${lim}`; do
            a=${arr[$i]}
            afn=$(basename "${a}")
            afn="${afn%.*}"
            b=${arr[$j]}
            bfn=$(basename "${b}")
            bfn="${bfn%.*}"
            ofn="${afn}-${bfn}.tsv"
            echo "python generate.py -A ${a} -B ${b} > ${DIR}/output/${ofn}"
            python generate.py -A ${a} -B ${b} > ${DIR}/output/${ofn}
        done
    fi
done    

